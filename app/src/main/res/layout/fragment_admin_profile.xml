<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".register_prof_student">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:gravity="center"
        android:background="@drawable/background_student_sign"
        >


        <Button
            android:id="@+id/levels_depart"
            android:layout_width="316dp"
            android:layout_height="60dp"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:layout_marginBottom="20sp"

            android:text="Levels &amp; Department"
            android:textAllCaps="false"
            android:background="@drawable/newstyle"
            android:textColor="#CE3A0143" />

        <Button
            android:id="@+id/professors"
            android:layout_width="316dp"
            android:layout_height="60dp"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:background="@drawable/newstyle"
            android:text="Professors  "
            android:textAllCaps="false"
            android:textColor="#CE3A0143" />

    </LinearLayout>

</FrameLayout>